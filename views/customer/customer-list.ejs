
   <%- include('../header.ejs') %>
   <!-- Layout wrapper -->
   <div class="layout-wrapper layout-content-navbar">
     <div class="layout-container">
       <!-- Menu -->
       <%- include('../menu.ejs') %>
       <!-- /Menu -->

       <!-- Layout container -->
       <div class="layout-page">
        <!-- Navbar -->
        <%- include('../navbar.ejs') %>
        <!-- /Navbar -->

         <!-- Content wrapper -->
        <div class="content-wrapper">
          <section class="notice">
            <div class="page-title">
                  <div class="container">
                      <h3>학생관리</h3>
                  </div>
              </div>
            
            <!-- board list area -->
              <div id="board-list">
                  <div class="container">
                    <div class="notice-act-section">
                    </div>
                      <table class="board-table">
                          <thead>
                          <tr>
                              <th scope="col" class="th-num">번호</th>
                              <th scope="col" class="th-id">아이디</th>
                              <th scope="col" class="th-name">이름</th>
                              <th scope="col" class="th-email">이메일</th>
                              <th scope="col" class="th-contact">연락처</th>
                          </tr>
                          </thead>
                          <tbody>
                            <% for(var i = 0; i < customers.length; i++){ %>
                                <tr>
                                    <td><%= customers[i].num %></td>
                                    <td><%= customers[i].id %></td>
                                    <td><%= customers[i].name %></td>
                                    <td><%= customers[i].email %></td>
                                    <td><%= customers[i].phonenumber %></td>
                                </tr>
                            <% } %>
                          </tbody>
                      </table>
                      
                  </div>
              </div>
          </section>
         <!-- Content wrapper -->
       </div>
       <!-- / Layout page -->
     </div>

     <!-- Overlay -->
     <div class="layout-overlay layout-menu-toggle"></div>
   </div>
   <%- include('../footer.ejs') %>
